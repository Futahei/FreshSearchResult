{
  "manifest_version": 2,
  "name": "FreshSearchResult",
  "description": "新鮮な情報の見分けをつけます",
  "version": "2.1.0",
  "icons": { "128": "image/icon128.png" },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
        "http://www.google.com/*"
      ],
      "js": [
        "js/main.js"
      ]
    }
  ]
}