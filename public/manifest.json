{
  "manifest_version": 2,
  "name": "FreshSearchResult",
  "description": "Visualize the freshness of the information.",
  "version": "3.1.3",
  "icons": { "128": "image/icon128.png" },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
        "http://www.google.com/*"
      ],
      "js": [
        "js/main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "image/bg/*.png",
    "image/bg/*.jpg"
  ]
}